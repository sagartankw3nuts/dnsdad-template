{
  "providerId": "secureserver.net",
  "providerName": "SecureServer",
  "serviceId": "cpanel",
  "serviceName": "cPanel",
  "version": 1,
  "description": "Enables a domain to work with GoDaddy Whitelabel cPanel",
  "shared": true,
  "sharedProviderName": true,      
  "variableDescription": "v1 is the IP address of the server.",
  "syncPubKeyDomain": "domain-attach.api.godaddy.com",
  "records": [
    {
      "pointsTo": "%v1%",
      "type": "A",
      "host": "@",
      "ttl": 3600
    },
    {
      "pointsTo": "%v1%",
      "type": "A",
      "host": "admin",
      "ttl": 3600
    },
    {
      "pointsTo": "%v1%",
      "type": "A",
      "host": "mail",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "www",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "cpanel",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "webdisk",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "whm",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "www.admin",
      "ttl": 3600
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "webdisk.admin",
      "ttl": 3600
    }
  ]
}
